import java.util.*;
class Inversionpair 
{
public static void main(String args[])
{
Scanner sc = new Scanner(System.in);
int m;
m = sc.nextInt();
int a[] = new int[m];
for(int i = 0;i<m;i++)
{
a[i] = sc.nextInt();
}
int c= 0;
for(int i = 0;i<m-1;i++)
{
for(int j = 1;j<m;j++)
{
if(a[i] > a[j])
{
c++;
}
}
}
System.out.println(c);
}
}
