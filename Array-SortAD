import java.util.*;
class SortAD
{
public static void main(String args[])
{
Scanner sc = new Scanner(System.in);
int m;
m = sc.nextInt();
int a[] = new int[m];
for(int i = 0;i<m;i++)
{
a[i] = sc.nextInt();
}
Arrays.sort(a);
for(int i = m-1;i>=m/2;i--)
{
System.out.println(a[i]+" ");
}
for(int i = 0;i<m/2;i++)
{
System.out.println(a[i]+" ");
}
}
}
